@{
    ViewBag.Title = "Home Page";
}

@using System.Diagnostics
@using BusinessLogic

@{


    SpiralGenerator sg = new SpiralGenerator();

    int[,] arr = sg.Generate(25);

    

<table class="table">
    @{
        int arraySize = arr.GetLength(0);

        for (int i = 0; i < arraySize; i++)
        {
            <tr>

                @for (int j = 0; j < arraySize; j++)
                {
                    <td>@arr[j, i]</td>
                }
            </tr>
        }
    }
</table>

}